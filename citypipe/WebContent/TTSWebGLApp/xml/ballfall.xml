				<translation_z>0</translation_z>
				<scale_x>1</scale_x>
				<scale_y>1</scale_y>
				<scale_z>1</scale_z>
				<rotate_x>1</rotate_x>
				<rotate_y>1</rotate_y>
				<rotate_z>1</rotate_z>
