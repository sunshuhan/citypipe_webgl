	<process>
		<subprocess name="ball2">
			<logic>1</logic>
			<time>  t &gt; 0</time>
			<position_x>0</position_x>
			<position_y>0</position_y>
			<position_z>0</position_z>
			<object>
				<id>4</id>
				<type>sphere</type>
				<radius>2</radius>
				<translation_x>-19</translation_x>
